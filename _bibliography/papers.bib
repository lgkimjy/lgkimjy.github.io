@article{gregor2015draw,
  abbr={Humanoids},
  abstract={The robot’s decision-making is an essential component of the autonomous robot. To fulfil this component, the accurate position estimation of the robot is a fundamental prerequisite. Localization determines the robot’s relative position within the map environment, and existing mobile robots have been extensively studied. However, localization is still challenging to humanoids because the bipeds’ movement is not as stable as mobile robots, and camera view frames oscillate from side to side. Developing localization with high accuracy under the above-limited constraints is necessary. This paper proposes an estimation of a 1.3m tall humanoid robot’s position with an adaptation of vision-based Augmented Monte-Carlo localization (aMCL) in the soccer field. Several approaches were also applied to enhance the localization performance. First, a deep learning-based object detection model was selected to identify pre-defined landmarks. In addition, the data association process was improved from the nearest neighbour matching algorithm to the Hungarian algorithm. This method enhanced data association performance, and the robot’s position was successfully estimated in real-time.},
  title={Enhancing AdultSize Humanoid Localization Accuracy: A Vision-based aMCL Leveraging Object Detection Model and Hungarian Algorithm},
  author={Kim, JunYoung and Ahn, Min Sung and Han, JeaKweon},
  journal={International Conference on Humanoid Robots (Humanoids)},
  year={2023},
  pdf={Humanoids_2023_JunYoungKim_FinalSubmit.pdf},
  video={https://www.youtube.com/watch?v=R78u-vm5boU},
}
html={https://lgkimjy.github.io/projects/0_humanoid_localization/},

@inproceedings{Kim2018-yo,
  abbr      = "한국정밀공학회",
  abstract  = "현대 사회가 초고령화 시대에 접어들게 됨에 따라 교통약자를 위한 사회 기반시설의 필요성은 점점 더 커지고 있다. 그 중 계단이나 경사로와 같이 고령자의 보행이 취약한 장소에 대한 시설물 대체는 공간적 시간적 비용적 문제가 크기 때문에 쉽지 않다. 그렇기 때문에 기존 시설물을 그대로 이용한 교통약자 개선 시스템의 개발은 이런 문제를 해결하기 위해 반드시 필요하다. 핸드레일은 계단이나 오르막 등에 낙상 방지 및 승강 편의를 위한 대표적 교통시설물이다. 따라서 이러한 공공 시설물에 몇 가지 장치를 추가하여 보행자의 편의를 제공할 수 있는 시스템 개발이 이루어지고 있다. 핸드레일을 이용한 보행보조 기기는 대부분 전동식 승강기 형태를 갖고 있으나, 핸드레일 창착형 보행보조 장치는 전동식 손잡이 형태로 일반적인 핸드레일에 구동용 보조레일을 장착하여 사용하는 방식이다. 이 장치는 여러 대의 기기를 사용할 수 있도록 자동 복귀 시스템을 가지고 있다. 복귀 시스템은 주행용, 복귀용 보조레일이 위/아래로 있고 양쪽 끝에 기기 승/하강을 위한 장치를 가지고 있다. 승/하강 장치는 보행보조 기기와 독립적으로 구동하기 때문에 다른 기기에 영향을 주지 않고 상호간에 정보를 주고받음으로써 기기 구동을 원활히 할 수 있도록 별도의 근거리 무선 통신장치가 필요하다. 또한 여러 대의 보행보조 기기 구동 시 이용자의 안전과 충돌을 방지하기 위한 장애물 탐지 센서가 필요하다. 이에 본 연구는 이 두 가지 기능을 통합한 새로운 센서 모듈을 제시하고자 한다.",
  title     = "[Poster] Development of a Sensor Module for Proximity Communication and Obstacle Detection of Handrail Mounted Type Walking Assists",
  booktitle = "Korean Society for Precision Engineering 학술발표대회 논문집",
  author    = "Kim, HyunJun and Kim, HyoJung and Oh, DoKyung and Kim, JunYoung and Kang, MinSung",
  pages     = "397--397",
  year      = "2018",
}
note      = "Poster presentation"

@inproceedings{Byun2017-ey,
  abbr      = "한국정밀공학회",
  abstract  = "가속화되는 고령화로 인해 보행에 어려움이 있는 고령자들에게 도움을 주는 보행보조기기의 사용이 증가하고 있고 이에 따른 보행보조기의 개발이 필요하다. 지금까지 개발되어 온 보행 보조 기술들은 경사로나 계단에서는 이동이 어렵고 개인이 사용하기에는 비용이 부담되므로 공공장소에 설치되어 있는 핸드 레일을 이용한 보행보조기기는 경사로 및 계단에서 이동이 쉽고 한번 설치 시 많은 사람이 이용할 수 있어서 유용하다. 핸드레일 장착형 보행보조기기는 기존에 설치되어 있던 핸드 레일에 보조 레일을 가설하여 핸드 레일에 기기를 장착하여 손잡이를 잡고 경사로나 계단을 오르는데 도움을 주는 전동식 장치이다. 이 장치에는 다음 사용자를 위한 시스템이 형성되어 있지  않기  때문에  이를  보완할  복귀  메커니즘을  추가하여  보행보조기기를  연속적으로  사용  할  수  있게  제안하고자 한다. 복귀 메커니즘은 승강기 형태로 이루어져 있으며 별도의 레일이 보조기기를 견인해주는 역할을 하며 순차적인 시스템을 통해 별도의 레일이 수직 상·하강하여 하단부에 가설된 레일을 타고 시작점으로 복귀가 이루어져 연속적으로 보행보조기기를 사용할 수 있게 설계하였다.",
  title     = "[Poster] Suggestion of Feedback Mechanism of a Walking Assistant with Handrail for the Elderly",
  booktitle = "Korean Society for Precision Engineering 학술발표대회 논문집",
  author    = "HyungJoo Byun and HyunJun Kim and HyoJung Kim and JunYoung Kim and Kang, MinSung",
  pages     = "317--317",
  year      = "2017",
}
note      = "Poster presentation"

@inproceedings{Kim2017-kr,
  abbr      = "한국정밀공학회",
  abstract  = "인간은 기본적으로 다양한 환경에서 그 상황에 적절한 동작을 비교적 쉽게 구현할 수 있다. 이는 인간이 기본적으로 습득한 동작들을 이용하고 응용할 수 있는 능력을 가지고 있기 때문이다. 그러나 로봇은 여러 상황에 따른 동작을 스스로 다양하게 구현하는 것이 쉬운 일은 아니다. 정확한 환경 및 동역학적 모델을 세우고 실제에 가까운 동작을 수행하기 위한 제어를 통해 다양한 동작을 구현할 수 있도록 하는데 이 또한 태스크나 환경에 제한적이라는 문제점이 있다. 따라서 본 논문에서는 사람이 기본적 운동들을 배우고 응용하는 것과 유사하게 가장 기본이 되는 동작을  연습함으로써  일반적인  동작을  할  수  있도록  하는  원시운동(movement  primitives)  학습방법을  이용한  2 자유도 로봇팔의 에어하키(air hockey)게임을 구현하고자 한다. 원시운동 학습방법은 사람에 의해 로봇 팔의 기본궤적을 직접 교시를 통해 학습을 하는 방법으로 태스크 수행 여부에 따른 보상을 통해 샘플 궤적을 얻어 다수의 상황에 적용할 수 있도록 일반화하는 방법이다. 따라서 다자유도 로봇을 짧은 시간 안에 동적인 운동을 학습 시킬 수 있는 효과적인 방법이다. 본 논문에서는 원시운동 학습방법의 효과를 에어하키에 적용함으로써 알아보고자 한다.",
  title     = "[Poster] Learning to Generate Trajectory of Striking Motion for Two-link Robot Arm in Air Hockey",
  author    = "HyoJung Kim and HyunJun Kim and DoKyung Oh and JunYoung Kim and HyungJoo Byun and Kang, MinSung",
  booktitle = "Korean Society for Precision Engineering 학술발표대회 논문집",
  pages     = "344--344",
  pdf={example.pdf},
  year      = "2017",
}
note      = "Poster presentation"